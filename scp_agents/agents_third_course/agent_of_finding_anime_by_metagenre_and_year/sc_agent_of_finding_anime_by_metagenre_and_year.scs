..sc_agent_of_finding_anime_by_metagenre_and_years_condition
= [*
	action_of_finding_anime_by_metagenre_and_year _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> rrel_1:: .._param1;;
	.._question _-> rrel_2:: .._param2;;
*];;

..sc_agent_of_finding_anime_by_metagenre_and_years_result
= [*
	action_of_finding_anime_by_metagenre_and_year _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> rrel_1:: .._param1;;
	.._question _-> rrel_2:: .._param2;;
*];;
